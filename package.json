{
  "name": "watson-conversation-demo-redesign",
  "version": "0.1.0",
  "description": "Watson Conversation demo on the Duo design system",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test-unit": "istanbul cover ./node_modules/mocha/bin/_mocha ./test/unit",
    "test-react": "./node_modules/jest/bin/jest.js --bail --findRelatedTests",
    "test": "npm run lint && npm run test-unit",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)",
    "validate": "npm ls",
    "watch-css": "node-sass -w src/scss/ -o public/style"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/Watson/watson-conversation-demo-redesign.git"
  },
  "keywords": [
    "Watson",
    "Conversation"
  ],
  "dependencies": {
    "async": "^2.5.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.17.2",
    "cf-deployment-tracker-client": "^0.x",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-browserify": "^1.0.2",
    "express-rate-limit": "^2.9.0",
    "express-status-monitor": "^1.0.0",
    "extend": "^3.0.1",
    "helmet": "^3.8.1",
    "jade": "^1.11.0",
    "jpath-query": "^0.1.1",
    "jquery-ui": "^1.12.1",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "uglifyify": "^4.0.3",
    "uuid": "^3.1.0",
    "watson-developer-cloud": "^2.39.0",
    "watson-react-components": "^0.6.12"
  },
  "devDependencies": {
    "casperjs": "^1.1.4",
    "codecov": "^2.3.0",
    "combinations": "^0.1.1",
    "enzyme": "^3.2.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^0.4.5",
    "jest": "^21.2.1",
    "mocha": "^3.5.0",
    "phantomjs-prebuilt": "^2.1.15",
    "precommit-hook": "^3.0.0",
    "supertest": "^3.0.0",
    "system": "^2.0.1"
