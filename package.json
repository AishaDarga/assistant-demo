{
  "name": "watson-conversation-demo-redesign",
  "version": "0.1.0",
  "description": "Watson Conversation demo on the Duo design system",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "npm run build && NODE_ENV=production node server.js",
    "build": "./node_modules/webpack/bin/webpack.js --config ./config/webpack.config.prod.js",
    "test-unit": "npm run build && istanbul cover ./node_modules/mocha/bin/_mocha ./test/unit",
    "test-react": "./node_modules/jest/bin/jest.js --config ./test/jest.config.json",
    "test": "npm run lint && npm run test-unit && npm run test-react",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)",
    "validate": "npm ls",
    "watch-css": "node-sass -w src/scss/ -o public/style"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/Watson/watson-conversation-demo-redesign.git"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/components/setupTests.js"
  },
  "keywords": [
    "Watson",
    "Conversation"
  ],
  "dependencies": {
    "@ibm/grid": "^0.2.3",
    "@ibm/type": "^0.5.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.17.2",
    "cf-deployment-tracker-client": "^0.x",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-rate-limit": "^2.9.0",
    "helmet": "^3.8.1",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "uglifyify": "^4.0.3",
    "uuid": "^3.1.0",
    "watson-developer-cloud": "^2.39.0",
    "watson-react-components": "^0.6.16",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "codecov": "^2.3.0",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^5.13.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "istanbul": "^0.4.5",
    "jest": "^21.2.1",
    "mocha": "^3.5.0",
    "postcss-sass": "^0.2.0",
    "precommit-hook": "^3.0.0",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "style-loader": "^0.19.0",
    "stylelint": "^8.3.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "supertest": "^3.0.0",
    "system": "^2.0.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "webpack-merge": "^4.1.1"
